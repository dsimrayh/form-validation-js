{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "quotes": [
        "error", 
        "single", 
        { "allowTemplateLiterals": true }
    ],
    "quote-props" : [
      "error",
      "consistent"
    ],
    "no-use-before-define": ["error", {
      "functions": false
    }],
    "no-param-reassign": 0
  }
}
